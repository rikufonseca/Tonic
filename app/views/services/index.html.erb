  <div class="all-frame margin-frame" data-navbar-target="pageup">
    <div class="inside-box">
      <div class="border-light-pink">
        <h3 class="underline-title"> <%= t('.mano') %> </h3>
      </div>
      <div class="border-light-pink">
        <h4 class="underline-title sub-title"> <%= t('.polish') %> </h4>
      </div>
      <div class="grid-container">
        <div class="grid-item">
          <% last_name = nil %>
          <% @services[:services_manicure_en].each do |service| %>
            <% if last_name != service.name_en %>
              <% last_name = service.name_en %>
              <h5 class="marge" <%= 'style="margin-top: 36px;"' if last_name == @services[:services_manicure_en].first.name_en %>>
                <%= service.name_en %>
              </h5>
            <% end %>
            <div class="inline">
              <p class="text"> <%= service.sub_name_en %> </p>
              <p> € <%= service.price %> </p>
            </div>
            <p class="left-aligned"> <%= service.description_en %> </p>
          <% end %>
        </div>
      </div>
      <div class="border-light-pink upper-line-pink">
        <h3 class="underline-title"> <%= t('.pedi') %></h3>
      </div>
      <div class="border-light-pink">
        <h4 class="underline-title sub-title"> <%= t('.polish') %> </h4>
      </div>
      <div class="grid-container">
        <div class="grid-item">
          <% count = 0 %>
          <% @services[:services_pedicure_en].each do | service | %>
              <h5 class="marge" <% if count == 0 %> style="margin-top: 36px;"<% end %>>
                <%= service.name_en %>
              </h5>
              <% count += 1 %>
              <div class="inline">
                <p class="text"> <%= service.sub_name_en %> </p>
                <p> € <%= service.price %> </p>
              </div>
              <p class="left-aligned"> <%= service.description_en %> </p>
          <% end %>
        </div>
      </div>
      <div class="border-light-pink upper-line-pink">
        <h3 class="underline-title"> <%= t('.men') %></h3>
      </div>
      <div class="grid-container">
        <div class="grid-item padding-out-tel">
        <% count = 0 %>
          <% @services[:services_men_en].each do | service | %>
              <h5 class="marge" <% if count == 0 %> style="margin-top: 36px;"<% end %>>
                <%= service.name_en %>
              </h5>
              <% count += 1 %>
              <div class="inline">
                <p class="text"> <%= service.sub_name_en %> </p>
                <p> € <%= service.price %> </p>
              </div>
              <p class="left-aligned"> <%= service.description_en %> </p>
          <% end %>
        </div>
      </div>
      <div class="border-light-pink upper-line-pink">
        <h3 class="underline-title"> NAILS ART </h3>
      </div>
      <div class="grid-container pad_all">
        <div class="grid-item padding-out-tel">
          <% @services[:services_art_en].each do | service | %>
              <div class="inline">
                <p class="text"> <%= service.sub_name_en %> </p>
                <p> <%= service.description_en %> € <%= service.price %> </p>
              </div>
          <% end %>
        </div>
      </div>
      <div class="border-light-pink upper-line-pink">
        <h3 class="underline-title"> EXTRA </h3>
      </div>
      <div class="grid-container pad_all">
        <div class="grid-item padding-out-tel">
          <% @services[:services_extra_en].each do | service | %>
            <div class="inline">
              <p class="text"> <%= service.sub_name_en %> </p>
              <p> <%= service.description_en %> € <%= service.price %> </p>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
